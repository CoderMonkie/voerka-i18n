{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"verbatimModuleSyntax": true,
		"strict": true,
		"module": "ES2022",
		"isolatedModules": true,
		"moduleResolution": "bundler"
	}
}
