{
    "name": "@voerkai18n/cli",
    "version": "2.1.13",
    "description": "命令行工具,用来初始化/提取/编译/自动翻译等工具链",
    "main": "src/index.js",
    "homepage": "https://gitee.com/zhangfisher/voerka-i18n",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zhangfisher/voerka-i18n"
    },
    "keywords": [
        "voerkai18n",
        "i18n",
        "language",
        "translation",
        "internationalize"
    ],
    "scripts": {
        "dev": "node src/index.js --",
        "translate": "node ./index.js translate",
        "extract": "node ./index.js extract -D -e templates/** ",
        "compile": "node ./index.js compile",
        "compile:en": "cross-env LANGUAGE=en node ./index.js compile -d",
        "release": "npm publish"
    },
    "author": "wxzhang",
    "license": "MIT",
    "bin": {
        "voerkai18n": "./index.js"
    },
    "dependencies": {
        "@babel/cli": "^7.17.6",
        "@babel/core": "^7.17.5",
        "@voerkai18n/runtime": "workspace:^",
        "@voerkai18n/utils": "workspace:^",
        "art-template": "^4.13.2",
        "axios": "^0.26.1",
        "bcp47-language-tags": "^1.0.7",
        "commander": "^9.0.0",
        "cross-env": "^7.0.3",
        "deepmerge": "^4.2.2",
        "flex-tools": "^1.4.27",
        "fs-extra": "^10.0.1",
        "glob": "^7.2.0",
        "gulp": "^4.0.2",
        "lcid": "^4.0.1",
        "locale-code": "^2.0.2",
        "logsets": "^1.3.14",
        "md5": "^2.3.0",
        "mixcli": "^3.2.4",
        "os-locale": "^6.0.2",
        "qs": "^6.10.3",
        "semver": "^7.3.7",
        "shelljs": "^0.8.5",
        "string.prototype.replaceall": "^1.0.7",
        "through2": "^4.0.2",
        "vinyl": "^2.2.1"
    },
    "devDependencies": {
        "@voerkai18n/runtime": "workspace:^",
        "tslib": "^2.8.1",
        "tsx": "^4.19.2"
    },
    "peerDependencies": {
        "@voerkai18n/runtime": "workspace:^"
    }
}
