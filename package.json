{
    "name": "voerka-i18n",
    "version": "2.0.0",
    "private": true,
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "cross-env NODE_OPTIONS=--experimental-vm-modules node node_modules/jest/bin/jest.js ",
        "test:app": "cross-env NODE_OPTIONS=--experimental-vm-modules node node_modules/jest/bin/jest.js -- app",
        "test:runtime": "cross-env  NODE_OPTIONS=--experimental-vm-modules jest runtime --coverage --collectCoverageFrom packages/runtime/dist/runtime.cjs",
        "build:all": "pnpm -r --filter \"runtime\" --filter \"react\"  --filter \"vue\" build ",
        "publish:all": "pnpm version:patch:all && pnpm build:all && git add . && git commit -m \"build: release all packages\" && pnpm -r  --filter \"./packages/**\" publish",
        "publish:all:noversion": "pnpm build:all && git add . && git commit -m \"build: release all packages\" && pnpm -r  --filter \"./packages/**\" publish",
        "version:patch:all": "pnpm -r --filter \"./packages/**\" version:patch ",
        "publish:all:beta": "pnpm version:patch:all && pnpm build:all && git add . && git commit -m \"build: release all packages\" && pnpm -r  --filter \"./packages/**\" publish --tag beta ",
        "sync": "cnpm sync @voerkai18n/cli @voerkai18n/runtime @voerkai18n/vue @voerkai18n/vite @voerkai18n/react @voerkai18n/utils @voerkai18n/vue2 @voerkai18n/babel voerkai18n-loader"
    },
    "author": "wxzhang@126.com",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.18.10",
        "@babel/plugin-transform-runtime": "^7.18.10",
        "@babel/preset-env": "^7.18.10",
        "@changesets/cli": "^2.26.2",
        "@modern-js/module-tools": "^2.24.0",
        "@rollup/plugin-babel": "^5.3.1",
        "@rollup/plugin-commonjs": "^21.1.0",
        "@vuepress/client": "2.0.0-beta.49",
        "commander": "^9.4.0",
        "cross-env": "^7.0.3",
        "dayjs": "^1.11.4",
        "deepmerge": "^4.2.2",
        "docsify": "^4.13.0",
        "dumi": "^1.1.47",
        "fs-extra": "^10.1.0",
        "gulp": "^4.0.2",
        "jest": "^27.5.1",
        "logsets": "^1.3.1",
        "pnpm": "^7.9.0",
        "prettier": "^2.7.1",
        "semver": "^7.3.7",
        "shelljs": "^0.8.5",
        "vinyl": "^2.2.1",
        "yalc": "1.0.0-pre.53",
        "yorkie": "^2.0.0"
    }
}
